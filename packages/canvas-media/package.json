{
  "name": "@instructure/canvas-media",
  "version": "1.8.0",
  "description": "A component that will handle upload/record and displaying of media",
  "author": "Instructure, Inc.",
  "main": "es/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "i18n:extract": "format-message extract \"src*/**/*.[jt]s*\" -g underscored_crc32 -o locales/en.json",
    "installTranslations": "scripts/installTranslations.js",
    "commitTranslations": "scripts/commitTranslations.sh",
    "test": "jest --color --runInBand",
    "jest": "jest --runInBand",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "build": "yarn build:canvas",
    "build:es": "babel src --out-dir es --ignore '**/__tests__'",
    "build:canvas": "yarn installTranslations && yarn run build:es",
    "build:watch": "yarn run clean:es && yarn run build:es --watch",
    "clean": "rm -rf lib && yarn run clean:es && rm -rf coverage",
    "clean:es": "rm -rf es",
    "prepublishOnly": "yarn build && yarn test",
    "publishToNpm": "scripts/publish_to_npm.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instructure/canvas-lms.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/instructure/canvas-lms/issues"
  },
  "homepage": "https://github.com/instructure/canvas-lms#readme",
  "dependencies": {
    "@instructure/emotion": "^8",
    "@instructure/k5uploader": "*",
    "@instructure/media-capture": "^9.0.0",
    "@instructure/ui-a11y-content": "^8",
    "@instructure/ui-alerts": "^8",
    "@instructure/ui-billboard": "^8",
    "@instructure/ui-buttons": "^8",
    "@instructure/ui-checkbox": "^8",
    "@instructure/ui-color-utils": "^8",
    "@instructure/ui-file-drop": "^8",
    "@instructure/ui-flex": "^8",
    "@instructure/ui-heading": "^8",
    "@instructure/ui-icons": "8",
    "@instructure/ui-media-player": "^9.0.0",
    "@instructure/ui-modal": "^8",
    "@instructure/ui-pagination": "^8",
    "@instructure/ui-progress": "^8",
    "@instructure/ui-react-utils": "^8",
    "@instructure/ui-select": "^8",
    "@instructure/ui-spinner": "^8",
    "@instructure/ui-svg-images": "^8",
    "@instructure/ui-tabs": "^8",
    "@instructure/ui-text": "^8",
    "@instructure/ui-text-input": "^8",
    "@instructure/ui-themes": "^8",
    "@instructure/ui-toggle-details": "^8",
    "@instructure/ui-tooltip": "^8",
    "@instructure/ui-utils": "^8",
    "@instructure/ui-view": "^8",
    "@instructure/uid": "8",
    "@sheerun/mutationobserver-shim": "^0.3.2",
    "aphrodite": "^2",
    "axios": "^0.21.1",
    "format-message": "^6",
    "format-message-generate-id": "^6",
    "lodash": "^4",
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@testing-library/dom": "^8",
    "@testing-library/react": "^12",
    "moxios": "^0.4",
    "shelljs": "^0.8.4",
    "sinon": "^7"
  }
}
