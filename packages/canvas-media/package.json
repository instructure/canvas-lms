{
  "name": "@instructure/canvas-media",
  "version": "1.1.0",
  "description": "A component that will handle upload/record and displaying of media",
  "author": "Instructure, Inc.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "jest --color --runInBand",
    "jest": "jest --runInBand",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "build": "yarn build:canvas",
    "build:es": "babel src --out-dir es --ignore '**/__tests__'",
    "build:cjs": "JEST_WORKER_ID=true babel src --out-dir lib --ignore '**/__tests__'",
    "build:canvas": "yarn run build:es && yarn run build:cjs",
    "build:watch": "yarn run build:es --watch",
    "clean": "rm -rf lib && rm -rf es && rm -rf coverage",
    "prepublishOnly": "yarn build && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instructure/canvas-lms.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/instructure/canvas-lms/issues"
  },
  "homepage": "https://github.com/instructure/canvas-lms#readme",
  "dependencies": {
    "@instructure/canvas-theme": "7",
    "@instructure/k5uploader": "^1.0.0",
    "@instructure/media-capture": "~8.0.1-rc.11",
    "@instructure/ui-a11y-content": "^7",
    "@instructure/ui-alerts": "^7",
    "@instructure/ui-billboard": "^7",
    "@instructure/ui-buttons": "^7",
    "@instructure/ui-checkbox": "^7",
    "@instructure/ui-color-utils": "^7",
    "@instructure/ui-file-drop": "^7",
    "@instructure/ui-flex": "^7",
    "@instructure/ui-icons": "7",
    "@instructure/ui-heading": "^7",
    "@instructure/ui-media-player": "^7.2",
    "@instructure/ui-modal": "^7",
    "@instructure/ui-pagination": "^7",
    "@instructure/ui-progress": "^7",
    "@instructure/ui-react-utils": "^7",
    "@instructure/ui-select": "^7",
    "@instructure/ui-spinner": "^7",
    "@instructure/ui-svg-images": "^7",
    "@instructure/ui-tabs": "^7",
    "@instructure/ui-text-input": "^7",
    "@instructure/ui-themeable": "^7",
    "@instructure/ui-themes": "^7",
    "@instructure/ui-utils": "^7",
    "@instructure/ui-text": "^7",
    "@instructure/ui-view": "^7",
    "@instructure/ui-toggle-details": "^7",
    "@instructure/uid": "7",
    "@sheerun/mutationobserver-shim": "^0.3.2",
    "axios": "^0.21.1",
    "format-message": "^6",
    "lodash": "^4",
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@testing-library/dom": "^7",
    "@testing-library/react": "^11",
    "@testing-library/user-event": "^5",
    "moxios": "^0.4",
    "sinon": "^7"
  }
}
