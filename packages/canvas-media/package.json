{
  "name": "@instructure/canvas-media",
  "version": "1.0.0",
  "description": "A component that will handle upload/record and displaying of media",
  "author": "Instructure, Inc.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "jest --runInBand",
    "jest": "jest --runInBand",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "build:es": "babel src --out-dir es --ignore '**/__tests__'",
    "build:cjs": "JEST_WORKER_ID=true babel src --out-dir lib --ignore '**/__tests__'",
    "build:canvas": "yarn run build:es && yarn run build:cjs",
    "build:watch": "yarn run build:es --watch",
    "clean": "rm -rf lib && rm -rf es && rm -rf coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instructure/canvas-lms.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/instructure/canvas-lms/issues"
  },
  "homepage": "https://github.com/instructure/canvas-lms#readme",
  "dependencies": {
    "@instructure/canvas-theme": "6",
    "@instructure/k5uploader": "^1.0.0",
    "@instructure/media-capture": "~7.1.0",
    "@instructure/ui-a11y": "6",
    "@instructure/ui-alerts": "6",
    "@instructure/ui-billboard": "6",
    "@instructure/ui-buttons": "6",
    "@instructure/ui-color-utils": "6",
    "@instructure/ui-elements": "6",
    "@instructure/ui-flex": "6",
    "@instructure/ui-forms": "6",
    "@instructure/ui-icons": "6",
    "@instructure/ui-heading": "6",
    "@instructure/ui-layout": "6",
    "@instructure/ui-media-player": "^5",
    "@instructure/ui-overlays": "6",
    "@instructure/ui-pagination": "6",
    "@instructure/ui-react-utils": "6",
    "@instructure/ui-select": "6",
    "@instructure/ui-svg-images": "6",
    "@instructure/ui-tabs": "6",
    "@instructure/ui-text-input": "6",
    "@instructure/ui-themeable": "6",
    "@instructure/ui-themes": "6",
    "@instructure/ui-utils": "6",
    "@instructure/ui-view": "6",
    "@instructure/ui-toggle-details": "6",
    "@instructure/uid": "6",
    "@sheerun/mutationobserver-shim": "^0.3.2",
    "axios": "^0.18.0",
    "format-message": "^6",
    "lodash": "^4",
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16"
