{{#if multipleDueDates}}
  <strong>{{#t "available"}}Available{{/t}}</strong>

  {{#if allDates}}
  <a title="tooltip"
     class="vdd_tooltip_link"
     aria-describedby="vdd_tooltip_{{selector}}"
     data-tooltip-selector="#vdd_tooltip_{{selector}}"
     {{#if linkHref}}href="{{linkHref}}"{{/if}}
     >{{#t "multiple_dates"}}Multiple Dates{{/t}}</a>

  <div id="vdd_tooltip_{{selector}}" style="display:none;">
    <dl class="vdd_tooltip_content dl-horizontal">
      {{#each allDates}}
        <div class="clearfix">
          <dt>{{dueFor}}</dt>
          <dd>
            <dd>
              {{#if available}}
                {{#t "available"}}Available{{/t}}
              {{/if}}

              {{>ui/shared/assignments/jst/_available_date_description.handlebars}}
            </dd>
          </dd>
        </div>
      {{/each}}
    </dl>
  </div>
  {{else}}
    {{#t "multiple_dates"}}Multiple Dates{{/t}}
  {{/if}}
{{else}}
  <span class="default-dates">
    {{#with defaultDates}}
      {{>ui/shared/assignments/jst/_available_date_description.handlebars}}
    {{/with}}
  </span>
{{/if}}
