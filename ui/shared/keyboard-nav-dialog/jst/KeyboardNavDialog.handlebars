<ul class="navigation_list" tabindex="-1">
  {{#each keyBindings}}
  <li>
    <span class="keycode">{{key}}</span>
    <span class="colon">:</span>
    <span class="description">{{desc}}</span>
  </li>
  {{/each}}
</ul>
<span>
  {{#t 'esc_key_notification'}}
    Press the escape key before using keyboard shortcuts when navigating the gradebook grid.
  {{/t}}
</span>
