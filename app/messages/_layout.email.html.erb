                                    <%= content(:footer_link).html_safe %>
                                  </span>
                              </td>
                          </tr>
                      </tbody></table>
                  </td>       
              </tr>
              <% end %>
              <!-- End Button-->
              <tr>
                <td align="left" bgcolor="#ffffff" style="padding: 0px 30px 0px 0px; color: #111111; font-family: 'Avenir', Helvetica, Arial, sans-serif; font-size: 18px; font-weight: 400; line-height: 25px;">&nbsp;</td>
              </tr>
              <!-- COPY -->
            </tbody></table>
            <!--[if (gte mso 9)|(IE)]>
            </td>
            </tr>
            </table>
            <![endif]-->
        </td>
  </tr>
    <!-- SUPPORT CALLOUT -->
