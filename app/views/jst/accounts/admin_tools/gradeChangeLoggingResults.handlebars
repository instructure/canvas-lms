{{#if collection.length}}

  <table class="table table-bordered">
    <thead>
      <tr>
        <td>{{#t "date"}}Date{{/t}}</td>
        <td>{{#t "time"}}Time{{/t}}</td>
        <td>{{#t "from"}}From{{/t}}</td>
        <td>{{#t "To"}}To{{/t}}</td>
        <td>{{#t "Grader"}}Grader{{/t}}</td>
        <td>{{#t "Student"}}Student{{/t}}</td>
        <td>{{#t "Course"}}Course{{/t}}</td>
        <td>{{#t "Assignment"}}Assignment{{/t}}</td>
      </tr>
    </thead>
    <tbody class="collectionViewItems"></tbody>
  </table>
  <div class="paginatedLoadingIndicator"></div>

{{else}}

  <div class="alert alert-info">
    <p class="lead">{{#t "no_item_found"}}No items found{{/t}}</p>
  </div>
  <div class="paginatedLoadingIndicator"></div>

{{/if}}
