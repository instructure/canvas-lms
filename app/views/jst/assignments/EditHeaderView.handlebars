<div class="header-bar-right">
  <div class="header-group-left">
    {{#if published}}
      <span id='assignment-draft-state' class='published'>
        <i class="icon-publish"></i>
        {{#t "buttons.published"}}Published{{/t}}
      </span>
    {{else}}
      <span id='assignment-draft-state' class='unpublished'>
        <i class="icon-unpublished"></i>
        {{#t "buttons.not_published"}}Not Published{{/t}}
      </span>
    {{/if}}
  </div>
  <div class="header-bar-right">
    <div class="admin-links">
      <a class="al-trigger btn">
        <span class="screenreader-only">Manage</span>
        <i class="icon-settings"></i><i class="icon-mini-arrow-down"></i>
      </a>
      <ul class="al-options">
        <li>
          <a href="#" class="delete_assignment_link">
            <i class="icon-trash"></i>
            {{#t "titles.delete"}}Delete{{/t}}
          </a>
        </li>
      </ul>
    </div>
  </div>
</div>
