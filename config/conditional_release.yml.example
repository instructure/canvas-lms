development:
  # required to enable the feature. default is false.
  enabled: false

  # required. no default.
  host: conditional_release.docker

  # optional. default matches Canvas
  protocol: http

  # optional. default is 'ui/editor'
  edit_rule_path: "ui/editor"

  # optional. default is 'api/account'
  create_account_path: 'api/account'
