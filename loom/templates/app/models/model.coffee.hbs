define ['vendor/ember/ember-model'], (Ember) ->

  # https://github.com/ebryn/ember-model

  attr = Ember.attr

  {{objectName}} = Ember.Model.extend

    id: attr()
    {{#each fields}}
    {{name}}: attr('{{type}}'){{comma}}
    {{/each}}
  {{objectName}}.reopenClass

    url: "/api/v1/???"

    adapter: Ember.RESTAdapter.create()

