<table>
  <!--Select to run with ePortfolios from users with no enrollments-->
  <tr>
    <td>
      <label class="checkbox">
        <%= check_box_tag "parameters[no_enrollments]"   %>
        <%= t('Only include ePortfolios from users with no enrollments') %>
      </label>
    </td>
  </tr>
  <!--Select to run with deleted users, courses, enrollments-->
  <tr>
    <td>
      <label class="checkbox">
        <%= check_box_tag "parameters[include_deleted]"   %>
        <%= t('Only include ePortfolios that have been removed') %>
      </label>
    </td>
  </tr>
</table>
