        <presentation>
          <flow>
            <material>
              <mattext texttype="text/html">&lt;p&gt;Solve the formula:&lt;/p&gt;</mattext>
              <mat_extension>
                <variable name="x">
                  <minvalue>10</minvalue>
                  <maxvalue>15</maxvalue>
                  <decimalplaces>3</decimalplaces>
                  <step>1</step>
                </variable>
                <variable name="y">
                  <minvalue>0.1</minvalue>
                  <maxvalue>0.9</maxvalue>
                  <decimalplaces>1</decimalplaces>
                  <step>0.1</step>
                </variable>
                <variable name="z">
                  <minvalue>100</minvalue>
                  <maxvalue>150</maxvalue>
                  <decimalplaces>0</decimalplaces>
                  <step>10</step>
                </variable>
                <formula>2 * {x} + {y} - {z}</formula>
              </mat_extension>
            </material>
            <response_num ident="QUES_979792_1194510_NUM" rcardinality="Single" rtiming="no">
              <render_fib fibtype="Decimal" prompt="Box">
                <response_label ident="QUES_979792_1194510_A1"/>
              </render_fib>
            </response_num>
            <response_str ident="QUES_979792_1194510_STR" rcardinality="Single" rtiming="no">
              <render_fib fibtype="String" prompt="Box">
                <response_label ident="QUES_979792_1194510_A2"/>
              </render_fib>
            </response_str>
          </flow>
        </presentation>
        <resprocessing>
          <respcondition title="Single Condition">
            <respcond_extension>
              <formula>2 * {x} + {y} - {z}</formula>
              <precision type="decimalplaces" d2l_2p0:precision_enforced="no">2
